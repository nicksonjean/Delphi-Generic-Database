    end;

    Dados2 := TArrayVariant.Create;
    SQL2 := TQuery.Create();
    try
      Dados2.Clear;
      SQL2 := Query2.View('SELECT * FROM app_empresa');
      Dados2['id'] := SQL2.Query.FieldByName('id').Value;
